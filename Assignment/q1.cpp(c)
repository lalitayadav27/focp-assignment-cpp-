#include <iostream>
using namespace std;
int main(){
  int n;
  cout<<"kindlly enter the num "<<endl;
  cin>>n;
  if
   int nextPrime = n + 1;
    while (true) {
        int i;
        for (i = 2; i * i <= nextPrime; i++) {
            if (nextPrime % i == 0) break; 
        }
        if (i * i > nextPrime) break; 
        nextPrime++;
    }

    cout << "Next higher prime: " << nextPrime << endl;
    return 0;
  
}
